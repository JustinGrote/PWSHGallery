{
  "configurations": [
    {
      "name": "Debug",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "console": "internalConsole",
      "runtimeExecutable": "npm",
      "presentation": {
        "hidden": false,
        "group": "npm",
        "order": 1
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2",
      "request": "launch",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "runtimeExecutable": "nvs",
      "cwd": "${workspaceFolder}",
      "args": [
        "run",
        "auto",
        "--experimental-vm-modules",
        "--no-warnings",
        "node_modules/jest/bin/jest.js",
        "--runInBand",
        "--watchAll=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ]
    }
  ]
}
